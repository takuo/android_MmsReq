<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >





<RelativeLayout android:id="@+id/RelativeLayout01" android:layout_height="fill_parent" android:layout_width="fill_parent"><TableLayout android:id="@+id/TableLayout01" android:layout_centerInParent="true" android:layout_centerVertical="true" android:layout_height="wrap_content" android:layout_width="fill_parent" android:stretchColumns="1" android:shrinkColumns="0"><TextView android:id="@+id/TextView01" android:layout_height="wrap_content" android:text="@string/description_label" android:layout_width="fill_parent" android:layout_marginLeft="5px"></TextView><TableRow android:layout_height="wrap_content" android:layout_width="fill_parent" android:baselineAligned="true" android:id="@+id/TableRow01"><Spinner android:layout_height="wrap_content" android:id="@+id/spinner_type" android:entries="@array/mms_type" android:layout_width="fill_parent"></Spinner>
<Button android:id="@+id/b_request" android:text="@string/b_request" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button></TableRow>

<TextView android:layout_height="wrap_content" android:id="@+id/t_result" android:layout_width="fill_parent" android:layout_marginLeft="5px"></TextView>

</TableLayout></RelativeLayout>
</LinearLayout>
